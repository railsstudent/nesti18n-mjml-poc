<mjml>
  <mj-head>
    <mj-attributes>
      <mj-class name="mjclass" width="600px" />
    </mj-attributes>
  </mj-head>
  <mj-body>
    <mj-section>
      <mj-column mj-class="mjclass">
        <mj-text font-size="30px"><%= title %></mj-text>
      </mj-column>
    </mj-section>
    <mj-section>
      <mj-column mj-class="mjclass">
        <mj-text><%= message %></mj-text>
      </mj-column>
    </mj-section>
    <mj-section>
      <mj-column mj-class="mjclass">
        <mj-text><%= admin %></mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>